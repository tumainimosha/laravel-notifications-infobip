{
    "name": "princeton255/laravel-notifications-infobip",
    "description": "A custom Laravel notifications channel for Infobip SMS API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tumaini Mosha",
            "email": "princeton.mosha@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "infobip/infobip-api-php-client": "^2.3"
    },
    "require-dev": {
        "illuminate/notifications": "^7.0@dev",
        "illuminate/support": "^7.0@dev",
        "illuminate/events": "^7.0@dev",
        "illuminate/queue": "^7.0@dev",
        "phpunit/phpunit": "^8.5@dev",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Infobip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Infobip\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
